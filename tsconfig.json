{
	"compilerOptions": {
		"target": "es2016",
		"module": "node16",
		"moduleResolution": "node16", /* Make sure dynamic imports are saved in their own chunks*/
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"jsx": "react" /* Specify JSX code generation: 'preserve', 'react-native', or 'react'. */,
		"jsxFactory": "m",
		"resolveJsonModule": true,
		"rootDir": "./src",
		"outDir": "./dist"
	}
}
